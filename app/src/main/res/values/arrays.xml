<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- attr_message的自定义xml属性-->
    <declare-styleable name="attr_message">
        <attr name="LeftText" format="reference|string"></attr>
        <attr name="RightText" format="reference|string"></attr>

        <!--View.VISIBLE-0-可见 View.INVISIBLE-4-不可见 View.GONE-8-隐藏-->
        <attr name="IconVisible">
            <enum name="visible" value="0"></enum>
            <enum name="invisible" value="4"></enum>
            <enum name="gone" value="8"></enum>
        </attr>

        <attr name="RightTextVisible">
            <enum name="visible" value="0"></enum>
            <enum name="invisible" value="4"></enum>
            <enum name="gone" value="8"></enum>
        </attr>

        <attr name="RightArrowVisible">
            <enum name="visible" value="0"></enum>
            <enum name="invisible" value="4"></enum>
            <enum name="gone" value="8"></enum>
        </attr>
    </declare-styleable>
    <!-- attr_gender_item的自定义xml属性-->


    <!-- attr_gender_item的自定义xml属性-->
    <declare-styleable name="attr_gender_item">
        <attr name="itemId" format="reference|string"></attr>
        <attr name="itemText" format="reference|string"></attr>
        <attr name="imageSrc" format="reference"></attr>
        <attr name="mtextColor" format="color"></attr>

        <!--false-0-没有选择.true-选择-->
        <attr name="isSelete">
            <enum name="mFalse" value="0"></enum>
            <enum name="mTure" value="1"></enum>
        </attr>

    </declare-styleable>    <!-- attr_gender_item的自定义xml属性-->
    <declare-styleable name="attr_examtime_time">
        <attr name="examTimeId" format="reference|string"></attr>
        <attr name="leftText" format="reference|string"></attr>

    </declare-styleable>

    <declare-styleable name="attr_login_register">

        <attr name="hintText" format="reference|string"></attr>
        <attr name="leftImageSrc" format="reference"></attr>
        <!--false-0-非密码.true-密码-->
        <attr name="isPassword">
            <enum name="mFalse" value="0"></enum>
            <enum name="mTure" value="1"></enum>
        </attr>
    </declare-styleable>

    <declare-styleable name="PickerDataView">

        <attr name="isLoop" format="boolean"/>

    </declare-styleable>

